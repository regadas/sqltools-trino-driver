{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "server": {
      "title": "Server",
      "type": "string"
    },
    "catalog": {
      "title": "Catalog",
      "type": "string"
    },
    "schema": {
      "title": "Schema",
      "type": "string"
    },
    "user": {
      "title": "User",
      "type": "string"
    },
    "password": {
      "title": "Password",
      "type": "string",
      "minLength": 1
    },
    "clientTags": {
      "title": "clientTags",
      "type": "string"
    },
    "trinoOptions": {
      "type": "object",
      "title": "Trino Options",
      "properties": {
        "ssl": {
          "type": "object",
          "title": "SSL",
          "properties": {
            "rejectUnauthorized": {
              "title": "Reject Unauthorized",
              "type": "boolean",
              "default": true
            }
          }
        }
      }
    }
  },
  "required": ["server", "catalog", "user"]
}
